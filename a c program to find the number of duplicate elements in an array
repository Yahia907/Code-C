#include<stdio.h>// a c program to find the total number of duplicate numbers in an array 
int main ()

{
int i,j,total_dup=0,n,arr[20];
printf("\n enter the number of elements in an array : ");
scanf("%d",&n);//reading number of the array elements 
for(i=0;i<n;i++)
{
printf("\n arr[%d]= ",i);//
scanf("%d",&arr[i]);//reading the array elements 
}
for(i=0;i<n;i++)
{
for(j=i+1;j<n;j++)
 { if(arr[i]==arr[j]&& i!=j)
 {
  total_dup++;
  break;
  printf("\n duplicates number :%d",total_dup);
 
 }
 }
 }
 printf("\n duplicates number :%d",total_dup);
 
 return 0;
 }
 